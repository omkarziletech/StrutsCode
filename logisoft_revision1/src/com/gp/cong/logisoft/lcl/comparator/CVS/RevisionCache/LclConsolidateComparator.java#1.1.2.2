package com.gp.cong.logisoft.lcl.comparator;

import com.gp.cong.logisoft.domain.lcl.LclConsolidate;
import java.util.Comparator;

/**
 *
 * @author lakshh
 */
public class LclConsolidateComparator implements Comparator<LclConsolidate> {

    public int compare(LclConsolidate lc1, LclConsolidate lc2) {
        if (lc1 == null || lc1.getLclFileNumberB().getId() == null) {
            return -1;
        }
        if (lc2 == null || lc2.getLclFileNumberB().getId() == null) {
            return 1;
        }
        return lc2.getLclFileNumberB().getId().compareTo(lc1.getLclFileNumberB().getId());
    }
}
