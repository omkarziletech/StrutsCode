                    <a id="lightBoxClose" href="javascript:closeCommentDiv();">
                        <img src="/logisoft/js/greybox/w_close.gif" title="Close" style="border: none;">Close
                    </a>
                </div>
            </td>
        </tr>
        <tr>
            <td>Comment:</td>
            <td>
                <textarea rows="2" cols="30" id="localDrayageComments"></textarea>
            </td>
        </tr>
        <tr>
            <td colspan="2" align="center">
                <input type="hidden" id="commentType">
                <input type="button" value="submit" onclick="submitComments('${QuoteValues.quoteId}','<%=userName %>','<%=todaysDate %>')">
                <input type="button" value="cancel" onclick="closeCommentDiv()">
            </td>
        </tr>
    </table>
</div>