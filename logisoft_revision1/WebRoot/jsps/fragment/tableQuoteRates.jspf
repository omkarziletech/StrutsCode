        if (d[h] == '\n' || d[h] == ' ') {
            yes = true;
        }

        if (Character.isLetter(d[h]) || Character.isDigit(d[h])) {
            s.append(d[h]);
        }
        if (yes == true) {
            s.append(" ");
            yes = false;
        }
    }

    if (s != null && s.toString().trim().length() == 0) {%>
                            <td></td>
                            <%} else {%>
                            <td align="center">
                                <span class="hotspot" onmouseover="tooltip.showComments('<%=s.toString()%>',null,event);"
                                      onmouseout="tooltip.hideComments();" style="color:black;">
                                <img src="<%=path%>/img/icons/view.gif"/></span>
