<%@ page language="java" import="java.util.*" pageEncoding="ISO-8859-1"%>
<%@page import="com.gp.cong.logisoft.util.CommonFunctions"%>
<%@page import="com.gp.cvst.logisoft.domain.FclBl"%>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
List displayId=(List)request.getAttribute("blList");
 %>
<%@ taglib uri="http://jakarta.apache.org/struts/tags-html" prefix="html" %>
<%@ taglib uri="/WEB-INF/displaytag-12.tld" prefix="display" %>
<%@ taglib uri="/WEB-INF/fmt.tld" prefix="fmt" %>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <base href="<%=basePath%>">
    
    <title>My JSP 'bl.jsp' starting page</title>
    
    
	<meta http-equiv="pragma" content="no-cache">
	<meta http-equiv="cache-control" content="no-cache">
	<meta http-equiv="expires" content="0">    
	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
	<meta http-equiv="description" content="This is my page">
	<!--
	<link rel="stylesheet" type="text/css" href="styles.css">
	-->
	 <script  language="javascript" type="text/javascript">
function showall(){
    document.searchQuotationform.buttonValue.value="correctedBL";
    document.searchQuotationform.submit();
}
</script>
  </head>
  
  <body>
    <html:form action="/searchquotation" name="searchQuotationform"
                   type="com.gp.cvst.logisoft.struts.form.SearchQuotationForm" scope="request">
                  
                     <input type="button" value="Search" onclick="showall()" class="buttonStyleNew"/>
                      <% int i=0;  %>
                      <display:table name="<%=displayId%>" id="fileNumberList" pagesize="${searchQuotationForm.limit}"
                                                       class="displaytagstyleNew" sort="list"  >
                                                       <%
                                                       String editPath="",bolid="";
                                                       if(CommonFunctions.isNotNullOrNotEmpty(displayId)){
                                                       FclBl fclBl =(FclBl)displayId.get(i);
                                                       bolid=fclBl.getBolId();
                                                       editPath= path + "/fclBL.do?paramid=" + fclBl.getBol();
                                                       } %>
                         <display:column title="ID" ><a href=<%=editPath%>><%=bolid %></a></display:column>
                         <%i++; %> 
                                                       </display:table>
    This is my JSP page. <br>
  </body>
   <html:hidden property="buttonValue"/>
    </html:form>
</html>
